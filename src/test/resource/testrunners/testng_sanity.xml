<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="RestAssured Framework" verbose="4" thread-count="1" parallel="tests">

	
	
	
	
	<test name="go Rest create HTTP call">
	<parameter name="baseURI" value="https://gorest.co.in" />
		<classes>
			
			<class name="com.qa.gorest.tests.CreateUserTest" />
			
		</classes>
	</test>
	
	<test name="fake store api get products">
	<parameter name="baseURI" value="https://fakestoreapi.com/" />
		<classes>
			
			<class name="com.qa.gorest.tests.FakeStoreProductsTest" />
			
		</classes>
	</test>
	
	
	

</suite>
